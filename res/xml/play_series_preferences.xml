<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <ListPreference
        android:key="playback_device_audio"
        android:title="@string/pref_playback_device_audio"
        android:entries="@array/pref_playback_device_titles"
        android:entryValues="@array/pref_playback_device_values"
        android:defaultValue="auto" />
    
    <ListPreference
        android:key="playback_device_video"
        android:title="@string/pref_playback_device_video"
        android:entries="@array/pref_playback_device_titles"
        android:entryValues="@array/pref_playback_device_values"
        android:defaultValue="auto" />

</PreferenceScreen>